<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="lhq.app.com.applicationtime">

    <uses-permission android:name="android.permission.PACKAGE_USAGE_STATS"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_time_icon"
        android:label="应用统计"
        android:roundIcon="@drawable/app_time_icon"
        android:supportsRtl="true"
        android:largeHeap="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AppStatisticsList"/>
        <activity android:name=".PiePolylineChartActivity"/>
        <activity android:name=".BarChartActivity"/>
        <activity android:name=".SplashActivity"/>
    </application>

</manifest>